{
  "name": "@v-charts2/radar",
  "version": "1.0.0",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.js"
    },
    "./vue3": {
      "import": "./src/index-vue3.js",
      "require": "./lib/vue3/v-charts.radar.umd.js"
    },
    "./vue2": {
      "import": "./src/index-vue2.js",
      "require": "./lib/vue2/v-charts.radar.umd.js"
    },
    "./main": "./src/main.js",
    "./v-charts.css": {
      "import": "./lib/style/v-charts.css",
      "require": "./lib/style/v-charts.css"
    }
  },
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "files": [
    "lib",
    "LICENSE"
  ],
  "scripts": {
    "build:lib": "yarn run clean && yarn run build:lib-all",
    "build:lib-all": "yarn run build:lib-vue3 & yarn run build:lib-vue2",
    "build:lib-vue2": "vite build --mode vue2",
    "build:lib-vue3": "vite build --mode vue3",
    "clean": "npx rimraf lib"
  },
  "dependencies": {
    "@v-charts2/core": "^1.0.0",
    "@v-charts2/pie": "^1.0.0",
    "echarts": "^5.3.0"
  },
  "devDependencies": {
    "rimraf": "^3.0.2",
    "rollup-plugin-copy": "^3.4.0",
    "vite": "^2.7.13",
    "vite-plugin-banner": "^0.2.0"
  },
  "peerDependencies": {
    "echarts": ">5.0.0",
    "vue": ">2.6.0 || >3.2.25"
  },
  "publishConfig": {
    "access": "public"
  }
}
