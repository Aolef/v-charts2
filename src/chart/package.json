{
  "name": "@v-charts2/chart",
  "version": "1.0.1",
  "description": "v-charts2 全部引入图表专用包",
  "keywords": [
    "echarts",
    "v-echarts",
    "v-charts"
  ],
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.js"
    },
    "./vue3": {
      "import": "./src/index-vue3.js",
      "require": "./lib/vue3/v-charts.chart.umd.js"
    },
    "./vue2": {
      "import": "./src/index-vue2.js",
      "require": "./lib/vue2/v-charts.chart.umd.js"
    },
    "./v-charts.css": {
      "import": "./lib/style/v-charts.css",
      "require": "./lib/style/v-charts.css"
    }
  },
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "files": [
    "lib",
    "LICENSE"
  ],
  "scripts": {
    "build:lib": "yarn run clean && yarn run build:lib-all",
    "build:lib-all": "yarn run build:lib-vue3 & yarn run build:lib-vue2",
    "build:lib-vue2": "vite build --mode vue2",
    "build:lib-vue3": "vite build --mode vue3",
    "clean": "npx rimraf lib"
  },
  "dependencies": {
    "@v-charts2/amap": "1.0.1",
    "@v-charts2/bar": "1.0.1",
    "@v-charts2/candle": "1.0.1",
    "@v-charts2/core": "1.0.1",
    "@v-charts2/funnel": "1.0.1",
    "@v-charts2/histogram": "1.0.1",
    "@v-charts2/line": "1.0.1",
    "@v-charts2/liquidfill": "1.0.1",
    "@v-charts2/map": "1.0.1",
    "@v-charts2/pie": "1.0.1",
    "@v-charts2/radar": "1.0.1",
    "@v-charts2/ring": "1.0.1",
    "@v-charts2/sankey": "1.0.1",
    "@v-charts2/scatter": "1.0.1",
    "@v-charts2/tree": "1.0.1",
    "@v-charts2/waterfall": "1.0.1"
  },
  "devDependencies": {
    "rimraf": "^3.0.2",
    "rollup-plugin-copy": "^3.4.0",
    "vite": "^2.7.13",
    "vite-plugin-banner": "^0.3.0"
  },
  "peerDependencies": {
    "echarts": ">5.0.0",
    "vue": ">2.6.0 || >3.2.25"
  },
  "engines": {
    "node": "^12.22.0 || ^14.17.0 || >=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "b57fb0006cfeb85b8cccd2ca1e9bf488686d71f6"
}
